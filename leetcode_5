// Last updated: 2025-11-02T21:41:22-08:00
// Last updated: 2025-11-03T21:41:02-08:00
bool isAnagram(char * s, char * t){
    int count[26] = {0};

    int n1 = strlen(s);
    int n2 = strlen(t);
    if (n1 != n2) return false;

    for (int i = 0; i < n1; i++) {
        count[s[i] - 'a']++;
        count[t[i] - 'a']--;
    }

    for (int i = 0; i < 26; i++)
        if (count[i] != 0) return false;

    return true;
}